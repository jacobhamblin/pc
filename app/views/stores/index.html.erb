<h3>Stores matching your requirements:</h3>

<table>
  <thead>
    <tr><td>Store</td><td>City</td></tr>
  </thead>
  <tbody>
    <% @stores.each do |store| %>
      <tr><td><%= store.name %></td><td><%= store.city.name %></tr>
    <% end %>
  </tbody>

  <%= @taco_names %>
  <%= @salsa_names %>
</table>
